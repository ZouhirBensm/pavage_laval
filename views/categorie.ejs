<!DOCTYPE html>
<!-- cant place alt lang page -->
<%- include('html/html-tag.ejs') %>


<head>

  <%- include('head/setup-pre-variables.ejs') %>
  
  <%- include('head/load-backend-variables/variables.ejs') %>
  
  <%- include('head/head-common-part1.ejs') %>
  
  <%- include('head/meta/meta-title-can.ejs') %>
  
  <%- include('head/css-loader/css.ejs') %>

  <%- include('head/meta/meta-lang-loader.ejs') %>
</head>






<body class="tm-bg-dark">


  <%- include('body2/nav.ejs') %>


  <main id="body">


    <div id="page-content">
      <div class="padding1">

        <div>
          <h2 class="category-template"><%= category.category_name %></h2>
          <h3>Blog Postings</h3>


            <ul>

              <% if (is_english) { %>

                <% blog_elements.forEach(function(blog) { %>
                  <li>
                    <a href="/blog/<%= category.slug.replace(/\/en$/, '') %>/blog-posting/<%= blog.slug %>" class="category-link">
                      <%= blog.title %>
                    </a>
                  </li>
                <% }); %>
              
              <% } else { %>

                <% blog_elements.forEach(function(blog) { %>
                  <li>
                    <a href="/blog/<%= category.slug %>/blog-posting/<%= blog.slug %>" class="category-link">
                      <%= blog.title %>
                    </a>
                  </li>
                <% }); %>

              <% } %>





            </ul>
        </div>

      </div>
    </div>





    <%- include('body2/brochure.ejs') %>



  </main>


  <%- include('footer/footer.ejs') %>



</body>




</html>